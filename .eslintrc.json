{
  "root": true,
  "extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "plugins": ["prettier", "react"],
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "rules": {
    "react/display-name": "off",
    "no-unused-vars": "warn"
  }
}
